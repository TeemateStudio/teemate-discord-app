{
  "name": "teemate-discord-app",
  "private": true,
  "version": "2.0.0",
  "description": "Teemate Discord bot with web dashboard",
  "main": "src/app.js",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "node src/app.js",
    "register": "node src/bot/commands.js",
    "dev": "nodemon src/app.js",
    "dev:dashboard": "cd dashboard && npm run dev",
    "dev:db": "docker compose -f docker-compose.dev.yml up -d",
    "dev:db:stop": "docker compose -f docker-compose.dev.yml down",
    "dev:all": "concurrently -n backend,dashboard -c blue,magenta \"npm run dev\" \"npm run dev:dashboard\""
  },
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "discord-interactions": "^4.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^8.9.0",
    "uuid": "^11.0.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.0"
  }
}
