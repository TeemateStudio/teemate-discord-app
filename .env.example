# Discord Bot Configuration
APP_ID=your_discord_app_id
DISCORD_TOKEN=your_discord_app_token
PUBLIC_KEY=your_discord_public_key
PORT=3000

# Discord OAuth2 (Dashboard)
CLIENT_SECRET=your_discord_client_secret
SESSION_SECRET=your_random_session_secret
# Local dev: http://localhost:5173 | Production: https://discord.teemate.gg
DASHBOARD_URL=http://localhost:5173

# MongoDB
# Local dev: mongodb://localhost:27017/teemate | Production: mongodb://mongodb:27017/teemate
MONGODB_URI=mongodb://localhost:27017/teemate

# Cloudflare Tunnel Token (not needed for local dev)
# TUNNEL_TOKEN=your_cloudflare_tunnel_token
